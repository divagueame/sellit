<h1>New Product</h1><br><br><br>
<div>

<%= form_with model: @product do |form| %>
<div>
    <%= form.label :title %>
    <%= form.text_field :title %>
</div>
<div>
    <%= form.label :description %>
    <%= form.text_area :description %>
</div>

<div>
    <%= form.label :price %>
    <%= form.number_field :price %>
</div>
<div>
    <%= form.submit %>
</div>
<% end %>
</div>